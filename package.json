{
  "name": "webgme-immortals",
  "author": "Fred Eisele",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "typings.json"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.isis.vanderbilt.edu:immortals/webgme-immortals.git"
  },
  "scripts": {
    "docs": "./node_modules/bin/jsdoc -c jsdoc_conf.json",
    "build": "tsc && npm run build:metadata",
    "build:metadata": "gulp dist-client-metadata",
    "start": "node ./node_modules/webgme/src/bin/start_server.js",
    "test": "node ./node_modules/mocha/bin/mocha --recursive test",
    "test_ci": "node ./node_modules/mocha/bin/mocha -R dot --timeout 10000 --recursive test",
    "test_cover": "node ./node_modules/istanbul/lib/cli.js --hook-run-in-context cover node_modules/mocha/bin/_mocha -- -R spec --timeout 10000 --recursive test",
    "test_browser": "node ./node_modules/karma/bin/karma start karma.conf.js --browsers Firefox --single-run --no-auto-watch",
    "postinstall": "bower install && npm run postinstall:n3",
    "postinstall:n3": "cd ./node_modules/n3 && browserify ./N3.js -s N3 -o ./browser/n3-browserify.js",
    "apply": "node ./node_modules/webgme/src/bin/apply.js",
    "diff": "node ./node_modules/webgme/src/bin/diff.js",
    "export": "node ./node_modules/webgme/src/bin/export.js",
    "import": "node ./node_modules/webgme/src/bin/import.js",
    "merge": "node ./node_modules/webgme/src/bin/merge.js",
    "plugin": "node ./node_modules/webgme/src/bin/run_plugin.js",
    "users": "node ./node_modules/webgme/src/bin/usermanager.js",
    "clean_up": "node ./node_modules/webgme/src/bin/clean_up.js",
    "stats": "node ./node_modules/webgme/src/bin/storage_stats.js",
    "gitclean": "rimraf ./dist-client",
    "restart": "sudo systemctl --system stop webgme.service"
  },
  "dependencies": {
    "amdefine": "github:allex/amdefine",
    "asyncawait": "^1.0.6",
    "bson": "^1.0.4",
    "bson-ext2": "^0.1.16",
    "cytoscape": "^3.1.2",
    "form-data": "^2.2.0",
    "fs-extra": "^3.0.1",
    "jquery": "^3.2.1",
    "mongodb": "^2.2.29",
    "n3": "^0.10.0",
    "no-kafka": "^3.1.8",
    "requirejs": "^2.3.3",
    "systemd-socket": "0.0.0",
    "transit-js": "file:../../transit-js",
    "text": "^0.1.0",
    "toposort": "github:phreed/toposort",
    "underscore": "^1.8.3",
    "webgme": "^2.14.1"
  },
  "devDependencies": {
    "@types/cytoscape": "github:phreed/ts-typings#cytoscape3",
    "@types/form-data": "^0.0.33",
    "@types/fs-extra": "^3.0.3",
    "@types/jquery": "^3.2.3",
    "@types/jszip": "^0.0.33",
    "@types/n3": "github:phreed/ts-typings#n3",
    "@types/node": "^8.0.1",
    "@types/requirejs": "^2.1.29",
    "@types/underscore": "^1.8.1",
    "bower": "^1.8.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "del": "^3.0.0",
    "glob-fs": "^0.1.6",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.7",
    "mocha": "^3.4.2",
    "node-gyp": "^3.6.2",
    "pm2": "^2.5.0",
    "rimraf": "^2.6.1",
    "ts-node": "^3.1.0",
    "tsify": "^3.0.1",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "vinyl-source-stream": "^1.1.0",
    "which": "^1.2.14"
  }
}
