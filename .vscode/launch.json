{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [       
        {
            "type": "node2",
            "request": "launch",
            "name": "Launch webGME",
            "program": "${workspaceRoot}/dist-client/main/app_bootstrap.js",
            "cwd": "${workspaceRoot}",
            "outFiles": [],
            "sourceMaps": true,
            "env": {
                "NODE_ENV": "immortals"
            }
        },
        {
            "type": "node2",
            "request": "launch",
            "name": "Launch Stream Plugin",
            "stopOnEntry": false,
            "program": "${workspaceRoot}/dist-client/main/run_plugin.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "StreamPlugin",
                "immortals_feature_model",
                "--owner",
                "fred",
                "--user",
                "fred",
                "--branchName",
                "master"
            ],
            "outFiles": ["${workspaceRoot}/output"],
            "sourceMaps": true,
            "env": {
                "NODE_ENV": "immortals"
            }
        },
        {
            "type": "node2",
            "request": "launch",
            "name": "Server Serialize Plugin",
            "stopOnEntry": false,
            "program": "${workspaceRoot}/dist-client/main/run_plugin.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "SerializerServerPlugin",
                "immortals_v04_mission_2017",
                "--owner",
                "fred",
                "--user",
                "fred",
                "--branchName",
                "master"
            ],
            "outFiles": ["${workspaceRoot}/output"],
            "sourceMaps": true,
            "env": {
                "NODE_ENV": "immortals"
            }
        },
        {
            "type": "node2",
            "request": "attach",
            "name": "Attach to Worker Process",
            "port": 5860,
            "outFiles": [],
            "sourceMaps": true
        },
         {
            "name": "Attach",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "url": "<url of the open browser tab to connect to>"
        },
        {
            "name": "Attach to url with files served from ./out",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "url": "<url of the open browser tab to connect to>",
            "webRoot": "${workspaceRoot}/out"
        }
    ]
}