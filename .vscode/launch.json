    "version": "0.2.0",
    "configurations": [       
        {
            "type": "node",
            "request": "launch",
            "name": "Launch webGME",
            "program": "${workspaceRoot}/dist-client/main/app_bootstrap.js",
            "cwd": "${workspaceRoot}",
            "outFiles": [],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch server",
            "program": "${workspaceRoot}/node_modules/webgme/src/bin/start_server.js",
            "cwd": "${workspaceRoot}",
            "outFiles": [],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Stream Plugin",
            "stopOnEntry": false,
            "program": "${workspaceRoot}/dist-client/main/run_plugin.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "StreamPlugin",
                "immortals_v03_cp02_network",
                "--owner",
                "fred",
                "--user",
                "fred",
                "--branchName",
                "master"
            ],
            "outFiles": ["${workspaceRoot}/output"],
            "sourceMaps": true,
            "env": {
                "NODE_ENV": "immortals"
            }
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858,
            "outFiles": [],
            "sourceMaps": true
        }
    ]
}