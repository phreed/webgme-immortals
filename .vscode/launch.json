    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch webGME",
            "program": "${workspaceRoot}/dist-client/app_bootstrap.js",
            "cwd": "${workspaceRoot}",
            "outFiles": [],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch server",
            "program": "${workspaceRoot}/node_modules/webgme/src/bin/start_server.js",
            "cwd": "${workspaceRoot}",
            "outFiles": [],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Stream Plugin",
            "program": "${workspaceRoot}/node_modules/webgme/src/bin/run_plugin.js",
            "cwd": "${workspaceRoot}/client",
            "args": [
                "StreamPlugin",
                "fred+immortals_v03_cp02_mission_2",
                "--branchName",
                "master"
            ],
            "outFiles": [],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858,
            "outFiles": [],
            "sourceMaps": true
        }
    ]
}