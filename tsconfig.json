{
  "atom": {
    "rewriteTsconfig": true
  },
  "compileOnSave": true,
  "buildOnSave": true,
  "compilerOptions": {
    "outDir": "./gen",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "module": "amd",
    "moduleResolution": "node",
    "removeComments": false,
    "target": "es3",
    "watch": true,
    "pretty": true,
    "noFallthroughCasesInSwitch": true
  },
  "filesGlob": [
    "./src-ts/**/*.ts",
    "!./node_modules/**/*.ts"
  ],
  "exclude": [
    "typings"
  ],
  "files": [
    "./src-ts/plugins/PushPlugin/PushPlugin.ts",
    "./src-ts/plugins/PushPlugin/text.d.ts",
    "./src-ts/plugins/StreamingPlugin/StreamingPlugin.ts",
    "./src-ts/serialize/NewSerializer.ts"
  ]
}
