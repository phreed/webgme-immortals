{
  "atom": {
    "rewriteTsconfig": true,
    "formatOnSave": true
  },
  "compileOnSave": true,
  "buildOnSave": true,
  "compilerOptions": {
    "outDir": "./gen",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "module": "amd",
    "moduleResolution": "node",
    "removeComments": false,
    "target": "es3",
    "watch": true,
    "pretty": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "rootDirs": [
      "./src/ts",
      "./src/js",
      "./gen"
    ],
    "paths": {
      "*": [
        "*",
        "./src/js/*",
        "./src/ts/*",
        "./gen/*"
      ]
    }
  },
  "filesGlob": [
    "./typings/index.d.ts",
    "./src/ts/**/*.ts",
    "!./node_modules/**",
    "!./**/*.spec.ts"
  ],
  "files": [
    "./typings/index.d.ts",
    "./src/ts/plugins/PushPlugin/PushPlugin.ts",
    "./src/ts/plugins/PushPlugin/text.d.ts",
    "./src/ts/plugins/StreamingPlugin/StreamingPlugin.ts",
    "./src/ts/serializer/NewSerializer.ts"
  ]
}
