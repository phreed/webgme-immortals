{
    "apps": [
        {
            "name": "webgme-immortals",
            "script": "./dist-client/app_bootstrap.js",
            "source_map_support": true,
            "instances": 1,
            "exec_mode": "fork_mode",
            "watch": [
                "dist-client"
            ],
            "ignore_watch": [
                "node_modules",
                "src"
            ],
            "watch_options": {
                "followSymlinks": false
            },
            "env": {
                "NODE_ENV": "development"
            },
            "env_production": {
                "NODE_ENV": "production"
            }
        }
    ]
}
